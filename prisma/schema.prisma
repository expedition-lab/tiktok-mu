generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Creator {
  id         Int     @id @default(autoincrement())
  name       String
  location   String?
  followers  Int      @default(0)
  engagement Float    @default(0)
  views      Int      @default(0)
  price      Int      @default(0)
  category   String?
  createdAt  DateTime @default(now())
}
